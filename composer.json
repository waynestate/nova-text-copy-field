{
    "name": "waynestate/nova-text-copy-field",
    "description": "Laravel Nova text field with click to copy support",
    "homepage": "https://github.com/waynestate/nova-text-copy-field",
    "keywords": [
        "laravel",
        "nova"
    ],
    "authors": [
        {
            "name": "Wayne State University",
            "email": "web@wayne.edu",
            "homepage": "https://wayne.edu"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.3.0",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Waynestate\\Nova\\TextCopy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waynestate\\Nova\\TextCopy\\FieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
